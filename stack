#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    //for implementing the stack
    stack<int>st;
    st.push(1);
    cout<<st.top();//1
    st.push(2);
    st.emplace(3);
    cout<<st.top();//3
    st.pop();   //top will be removed
    st.size();
    cout<<st.empty();//0
    //stack<int>st1,st2;
    //st1.swap(st2);
}
